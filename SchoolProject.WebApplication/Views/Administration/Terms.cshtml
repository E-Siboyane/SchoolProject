@model IEnumerable<SchoolProject.WebApplication.Models.AdminTerm>

@{
    ViewBag.Title = "Terms";
}

<h2>Performance Measure Terms</h2>

<table id="datatable" class="table table-hover">
    <thead>
        <tr>
            <th>
                @Html.Raw("Term Descrciption")
            </th>
            <th>
                @Html.Raw("Code")
            </th>
          
        </tr>
        </thead>
    <tbody>
        @foreach (var item in Model) {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.TermName)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.TermCode)
                </td>
            </tr>
        }
        </tbody>
<tfoot>
        <tr>
            <th>
                @Html.Raw("Term Descrciption")
            </th>
            <th>
                @Html.Raw("Code")
            </th>
           
        </tr>
    </tfoot>
</table>
